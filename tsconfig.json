{
  "compilerOptions": {
    "target": "es2018",
    "module": "commonjs",
    "removeComments": false,
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "outDir": "lib",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "baseUrl": ".",
    "rootDir": ".",
    "jsx": "react",
    "jsxFactory": "React.createElement",
    "allowJs": true,
    "resolveJsonModule": true,
    "typeRoots": [
      "node_modules/@types",
      "global.d.ts"
    ],
    "paths": {
      "@/pages/*": ["./src/pages/*"],
      "@/subPackage/*": ["./src/subPackage/*"],
      "@/models/*": ["./src/models/*"],
      "@/assets/*": ["./src/assets/*"],
      "@/image/*": ["./src/image/*"],
      "@/config/*": ["./src/config/*"],
      "@/components/*": ["./src/components/*"],
      "@/service/*": ["./src/service/*"],
      "@/utils/*": ["./src/utils/*"],
      "@/package": ["./package.json"],
      "@/project": ["./project.config.json"]
    }
  },
  "exclude": [
    "node_modules",
    "dist"
  ],
  "compileOnSave": false
}
